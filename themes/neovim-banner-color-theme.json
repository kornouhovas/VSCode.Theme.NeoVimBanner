  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5c6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "entity.name.class",
        "entity.name.struct",
        "entity.name.interface"
      ],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#e06c75",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Namespace",
      "scope": [
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#61afef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#e5c07b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#c678dd",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Inline Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link",
        "meta.link"
      ],
      "settings": {
        "foreground": "#56b6c2",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Lists",
      "scope": [
        "markup.list",
        "meta.list"
      ],
      "settings": {
        "foreground": "#c8ccd4"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Diff Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#e06c75",
        "fontStyle": "italic"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#56b6c2",
    "type": "#e5c07b",
    "class": "#e5c07b",
    "interface": "#e5c07b",
    "enum": "#e5c07b",
    "typeParameter": "#c678dd",
    "parameter": "#e06c75",
    "property": "#56b6c2",
    "variable": "#e06c75",
    "function": "#61afef",
    "method": "#61afef",
    "macro": "#c678dd",
    "keyword": "#c678dd",
    "comment": "#5c6370",
    "string": "#98c379",
    "number": "#d19a66",
    "regexp": "#56b6c2"
  }
}
